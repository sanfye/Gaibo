<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<!-- 命名空间为DAO接口的路径，之前是domain对象的路径 -->
<mapper namespace="com.gaibo.biz.dao.IWarningDao" >


	<select id="queryWarning"  resultType="com.gaibo.biz.share.vo.WarningFromDb" >
		select 
		    u.id,
		    u.warningId,
		    u.warningEnglish,
		    u.warningChinese
		 from   gable.warning u
		  
	</select>	

	
</mapper>	